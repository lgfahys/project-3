{
  "name": "mern",
  "version": "1.0.0",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "aws-sdk": "^2.489.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "bootstrap": "^4.3.1",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "file-type": "^12.0.1",
    "fs": "0.0.1-security",
    "geolib": "^3.0.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.5",
    "multiparty": "^4.2.1",
    "npm": "^6.10.1",
    "path": "^0.12.7",
    "react-card-flip": "^0.8.0",
    "react-s3": "^1.3.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.5"
  }
}
